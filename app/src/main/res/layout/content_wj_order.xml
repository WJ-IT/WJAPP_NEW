<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/activity_wj_order"
        tools:context=".WjOrderActivity"
        android:id="@+id/cl_ordermain">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:id="@+id/linearLayout6"
            android:gravity="center"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="4dp"
            android:layout_marginTop="5dp">

        <TextView
                android:text="주문제품"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView50"
                android:textColor="@color/dialog_top"
                android:textSize="14dp"/>

        <TextView
                android:text="카테고리"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/textView51"
                android:textColor="@color/dialog_top"
                android:textSize="14dp"/>
    </LinearLayout>

    <android.support.v7.widget.RecyclerView
            android:layout_width="0dp"
            android:layout_height="50dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:id="@+id/rvWjOrder"
            app:layout_constraintStart_toEndOf="@+id/linearLayout6"
            android:layout_marginStart="8dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="8dp">

    </android.support.v7.widget.RecyclerView>

    <TableLayout
            android:id="@+id/tlNmItem"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="2dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="2dp"
            android:background="@drawable/gradation_background1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/rvWjOrder">

        <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            <TextView
                    android:id="@+id/textView36"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="30"
                    android:gravity="end"
                    android:text="주문제품  "
                    android:textColor="#FFFFFF"
                    android:textSize="20dp" />

            <Spinner

                android:id="@+id/spin_nmItem"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="70"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />
        </TableRow>

    </TableLayout>

    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/tlNmItem"
            android:gravity="center"
            android:id="@+id/linearLayout17"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="5dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="5dp"
            android:layout_marginTop="5dp">

        <TableLayout
                android:id="@+id/tlOrder"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:layout_editor_absoluteY="85dp">


            <TableRow
                    android:id="@+id/trV2Dia"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingBottom="5dp">

                <android.support.constraint.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                    <TextView
                            android:id="@+id/textView38"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="21dp"
                            android:text="Diameter"
                            android:textColor="#4C4C4C"
                            android:textStyle="italic"
                            app:layout_constraintEnd_toStartOf="@+id/txt_bc"
                            app:layout_constraintHorizontal_bias="0.5"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                    <Spinner

                        android:id="@+id/spin_specDia"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        app:layout_constraintEnd_toStartOf="@+id/spin_specBc"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView38"
                        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                    <TextView
                            android:id="@+id/textView151"
                            android:layout_width="0dp"
                            android:layout_height="1dp"
                            android:layout_marginStart="16dp"
                            android:background="#3898E1"
                            app:layout_constraintEnd_toStartOf="@+id/txt_bc_under"
                            app:layout_constraintHorizontal_bias="0.5"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/spin_specDia" />

                    <TextView
                            android:id="@+id/txt_bc"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="48dp"
                            android:layout_marginEnd="16dp"
                            android:layout_weight="20"
                            android:text="Base Curve"
                            android:textColor="#4C4C4C"
                            android:textStyle="italic"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.5"
                            app:layout_constraintStart_toEndOf="@+id/textView38"
                            app:layout_constraintTop_toTopOf="parent" />

                    <Spinner

                        android:id="@+id/spin_specBc"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginStart="48dp"
                        android:layout_marginEnd="16dp"
                        android:layout_weight="35"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/spin_specDia"
                        app:layout_constraintTop_toBottomOf="@+id/textView38"
                        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                    <TextView
                            android:id="@+id/txt_bc_under"
                            android:layout_width="0dp"
                            android:layout_height="1dp"
                            android:layout_marginStart="48dp"
                            android:layout_marginEnd="16dp"
                            android:background="#3898E1"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.5"
                            app:layout_constraintStart_toEndOf="@+id/textView151"
                            app:layout_constraintTop_toBottomOf="@+id/spin_specBc" />
                </android.support.constraint.ConstraintLayout>

            </TableRow>

            <TableRow
                    android:id="@+id/trV3Bc"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingBottom="5dp">

                <android.support.constraint.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                </android.support.constraint.ConstraintLayout>

            </TableRow>

            <TableRow
                    android:id="@+id/trV4Power"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingBottom="5dp">

                <android.support.constraint.ConstraintLayout
                        android:id="@+id/cl_power"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                    <TextView
                            android:id="@+id/txt_power"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="21dp"
                            android:text="Power"
                            android:textColor="#4C4C4C"
                            android:textStyle="italic"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                    <Spinner

                        android:id="@+id/spin_specPower"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginStart="16dp"
                        app:layout_constraintEnd_toStartOf="@+id/txtFrToPower"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/txt_power"
                        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                    <TextView
                            android:id="@+id/txtFrToPower"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:text="~"
                            android:textSize="20dp"
                            android:textStyle="bold"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/txt_power" />

                    <Spinner

                        android:id="@+id/spin_specPower2"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginEnd="16dp"
                        android:layout_weight="35"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/txtFrToPower"
                        app:layout_constraintTop_toBottomOf="@+id/txt_power"
                        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                    <TextView
                            android:id="@+id/textView184"
                            android:layout_width="0dp"
                            android:layout_height="1dp"
                            android:layout_marginStart="16dp"
                            android:layout_marginEnd="16dp"
                            android:background="#3898E1"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/txtFrToPower" />
                </android.support.constraint.ConstraintLayout>

            </TableRow>

            <TableRow
                    android:id="@+id/trV5Spec"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingBottom="5dp">

                <android.support.constraint.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                    <TextView
                            android:id="@+id/txt_spec"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="21dp"
                            android:text="Spec"
                            android:textColor="#4C4C4C"
                            android:textStyle="italic"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                    <Spinner

                        android:id="@+id/spin_specSpec"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginStart="16dp"
                        android:layout_marginEnd="16dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/txt_spec"
                        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                    <TextView
                            android:id="@+id/textView158"
                            android:layout_width="0dp"
                            android:layout_height="1dp"
                            android:layout_marginStart="16dp"
                            android:layout_marginEnd="16dp"
                            android:background="#3898E1"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/spin_specSpec" />
                </android.support.constraint.ConstraintLayout>

            </TableRow>

            <TableRow
                    android:id="@+id/trDtSurg"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingBottom="5dp">

                <android.support.constraint.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                    <TextView
                            android:id="@+id/textView63"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="21dp"
                            android:text="수술일자"
                            android:textColor="#4C4C4C"
                            android:textStyle="italic"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                            android:id="@+id/txt_dt_surg"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="16dp"
                            android:layout_marginEnd="16dp"
                            android:layout_weight="80"
                            android:gravity="center"
                            android:hint="터치하세요"
                            android:textColor="#000000"
                            android:textSize="18dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/textView63" />

                    <TextView
                            android:id="@+id/textView183"
                            android:layout_width="0dp"
                            android:layout_height="1dp"
                            android:layout_marginStart="16dp"
                            android:layout_marginEnd="16dp"
                            android:background="#3898E1"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/txt_dt_surg" />
                </android.support.constraint.ConstraintLayout>

            </TableRow>

        </TableLayout>

    </LinearLayout>

    <android.support.constraint.ConstraintLayout
            android:id="@+id/constraintLayout5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="21dp"
            android:layout_marginEnd="21dp"
            android:background="@drawable/bg_all_round"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout17">

        <TextView
                android:id="@+id/textView60"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:text="주문수량"
                android:textColor="#4C4C4C"
                android:textStyle="italic"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/txt_qty"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_weight="36"
            android:background="@null"
            android:ems="10"
            android:gravity="center"
            android:hint="0"
            android:inputType="numberDecimal"
            android:textColor="#4CAF50"
            android:textSize="20dp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView60"
            tools:ignore="TouchTargetSizeCheck" />

        <Button
            android:id="@+id/btn_jaego"
            android:layout_width="60dp"
            android:layout_height="30dp"
            android:layout_marginStart="48dp"
            android:layout_weight="21"
            android:background="@drawable/search_background_slim"
            android:text="재고확인"
            android:textColor="@android:color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/txt_qty"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="TouchTargetSizeCheck" />

        <TextView
                android:id="@+id/txt_jaegoQty"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:textColor="@android:color/holo_blue_dark"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/btn_jaego"
                app:layout_constraintTop_toTopOf="parent" />

    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
            android:id="@+id/constraintLayout6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="21dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="21dp"
            android:background="@drawable/bg_all_round"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout5">

        <TextView
                android:id="@+id/textView61"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_weight="24"
                android:text="특이사항"
                android:textColor="#4C4C4C"
                android:textStyle="italic"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/txt_remark"
            android:layout_width="0dp"
            android:layout_height="25dp"
            android:background="@null"
            android:ems="10"
            android:gravity="center"
            android:hint="특이사항 입력"
            android:singleLine="true"
            android:textColor="#000000"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView61"
            tools:ignore="TouchTargetSizeCheck" />

    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
            android:id="@+id/constraintLayout7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="21dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="21dp"
            android:background="@drawable/bg_all_round"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout6">

        <TextView
                android:id="@+id/textView64"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:text="환자명"
                android:textColor="#4C4C4C"
                android:textStyle="italic"
                app:layout_constraintEnd_toStartOf="@+id/textView188"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/txt_patient"
            android:layout_width="0dp"
            android:layout_height="25dp"
            android:background="@null"
            android:ems="10"
            android:gravity="center"
            android:hint="직배송시 필수"
            android:singleLine="true"
            android:textColor="#000000"
            android:textSize="14sp"
            app:layout_constraintEnd_toStartOf="@+id/txt_addr_hp"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView64"
            tools:ignore="TouchTargetSizeCheck" />

        <TextView
                android:id="@+id/textView188"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="37dp"
                android:text="휴대폰 번호"
                android:textColor="#4C4C4C"
                android:textStyle="italic"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/textView64"
                app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/txt_addr_hp"
            android:layout_width="0dp"
            android:layout_height="25dp"
            android:layout_marginStart="32dp"
            android:background="@null"
            android:ems="10"
            android:gravity="center"
            android:hint="직배송시 필수"
            android:inputType="numberSigned"
            android:textColor="#000000"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/txt_patient"
            app:layout_constraintTop_toBottomOf="@+id/textView64"
            tools:ignore="TouchTargetSizeCheck" />

    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
            android:id="@+id/constraintLayout8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="21dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="21dp"
            android:background="@drawable/bg_all_round"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout7">

        <TextView
                android:id="@+id/textView65"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:text="배송주소"
                android:textColor="#4C4C4C"
                android:textStyle="italic"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/btn_zipcode"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:background="@drawable/search_background_slim"
            android:text="우편번호 검색"
            android:textColor="#FFFFFF"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="TouchTargetSizeCheck" />

        <EditText
            android:id="@+id/txt_addr"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:background="@null"
            android:editable="false"
            android:ems="10"
            android:gravity="center"
            android:hint="기본주소(검색)"
            android:textColor="#000000"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView65"
            tools:ignore="TouchTargetSizeCheck" />

        <EditText
            android:id="@+id/txt_addr_detail"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:background="@null"
            android:ems="10"
            android:gravity="center"
            android:hint="상세주소 입력"
            android:inputType="textPersonName"
            android:singleLine="true"
            android:textColor="#000000"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txt_addr"
            tools:ignore="TouchTargetSizeCheck" />

    </android.support.constraint.ConstraintLayout>

    <Button
        android:id="@+id/btn_addr_del"
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:layout_marginEnd="24dp"
        android:background="@null"
        android:singleLine="true"
        android:text="배송정보 삭제"
        android:textColor="#154EF6"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout8"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/btn_basket"
        android:layout_width="0dp"
        android:layout_height="41dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/search_background_slim_flat"
        android:backgroundTintMode="multiply"
        android:text="장바구니 넣기"
        android:textColor="@android:color/white"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/btn_basket_open"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_addr_del"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/btn_basket_open"
        android:layout_width="130dp"
        android:layout_height="41dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/search_background_slim_flat2"
        android:text="장바구니 확인"
        android:textColor="#000000"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_addr_del"
        tools:ignore="TouchTargetSizeCheck" />

    <android.support.constraint.ConstraintLayout
            android:id="@+id/clBasket"
            android:layout_width="10dp"
            android:layout_height="30dp"
            android:layout_marginBottom="150dp"
            android:animateLayoutChanges="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

        <TextView
                android:id="@+id/textView62"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginStart="4dp"
                android:layout_marginEnd="4dp"
                android:alpha=".6"
                android:background="@color/dialog_top"
                android:textSize="14dp"
                app:layout_constraintBottom_toBottomOf="@+id/textView54"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textView54" />

        <TextView
                android:id="@+id/txt_endBar"
                android:layout_width="10dp"
                android:layout_height="0dp"
                android:background="@drawable/search_background"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        <TextView
                android:id="@+id/textView58"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:alpha=".3"
                android:background="@color/dialog_top"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/txt_endBar"
                app:layout_constraintStart_toEndOf="@+id/txt_basketBar"
                app:layout_constraintTop_toTopOf="parent" />

        <TextView
                android:id="@+id/textView54"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="80"
                android:alpha="1"
                android:gravity="center"
                android:text="장바구니"
                android:textColor="@android:color/white"
                android:textSize="30dp"
                app:layout_constraintEnd_toEndOf="@+id/textView58"
                app:layout_constraintStart_toEndOf="@+id/txt_basketBar"
                app:layout_constraintTop_toTopOf="parent" />

        <TableLayout
                android:id="@+id/tlBasket"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="9dp"
                android:layout_marginEnd="9dp"
                android:background="@drawable/search_background"
                app:layout_constraintEnd_toStartOf="@+id/txt_endBar"
                app:layout_constraintStart_toEndOf="@+id/txt_basketBar"
                app:layout_constraintTop_toBottomOf="@+id/textView54">

            <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                <TextView
                        android:id="@+id/textView55"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="80"
                        android:gravity="center"
                        android:text="제품명 / 규격"
                        android:textColor="@android:color/white"
                        android:textSize="14dp" />

                <TextView
                        android:id="@+id/textView59"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="10"
                        android:gravity="center"
                        android:text="수량"
                        android:textColor="@android:color/white"
                        android:textSize="14dp" />

                <TextView
                        android:id="@+id/textView66"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="10"
                        android:gravity="center"
                        android:text="오더"
                        android:textColor="@android:color/white"
                        android:textSize="14dp" />

                <TextView
                        android:id="@+id/textView56"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="10"
                        android:textSize="14dp" />
            </TableRow>

            <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                <TextView
                        android:id="@+id/textView67"
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1"
                        android:background="@drawable/dot"
                        android:text="TextView"
                        android:textSize="14dp" />
            </TableRow>

            <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                <TextView
                        android:id="@+id/textView68"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="15"
                        android:gravity="center"
                        android:text="환자명"
                        android:textColor="@android:color/white"
                        android:textSize="14dp" />

                <TextView
                        android:id="@+id/textView69"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="85"
                        android:gravity="center"
                        android:text="특이사항/배송주소"
                        android:textColor="@android:color/white"
                        android:textSize="14dp" />
            </TableRow>

        </TableLayout>

        <android.support.v7.widget.RecyclerView
                android:id="@+id/rvWjBasket"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginStart="9dp"
                android:layout_marginEnd="9dp"
                android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toTopOf="@+id/btn_order"
                app:layout_constraintEnd_toStartOf="@+id/txt_endBar"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tlBasket">

        </android.support.v7.widget.RecyclerView>

        <Button
                android:id="@+id/btn_all_delete"
                android:layout_width="wrap_content"
                android:layout_height="41dp"
                android:layout_marginStart="12dp"
                android:layout_marginBottom="2dp"
                android:alpha=".8"
                android:background="@drawable/search_background_slim_flat"
                android:text="전체삭제"
                android:textColor="@android:color/white"
                android:textSize="18dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

        <Button
                android:id="@+id/btn_order"
                android:layout_width="wrap_content"
                android:layout_height="41dp"
                android:layout_marginEnd="12dp"
                android:layout_marginBottom="2dp"
                android:alpha=".8"
                android:background="@drawable/search_background_slim_flat"
                android:text="주문하기"
                android:textColor="@android:color/white"
                android:textSize="18dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" />

        <TextView
                android:id="@+id/txt_basketBar"
                android:layout_width="10dp"
                android:layout_height="0dp"
                android:alpha="1"
